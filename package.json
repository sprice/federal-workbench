    "db:studio": "drizzle-kit studio",
    "db:push": "drizzle-kit push",
    "db:pull": "drizzle-kit pull",
    "db:check": "drizzle-kit check",
    "db:up": "drizzle-kit up",
    "test:embeds": "npx tsx scripts/test-search.ts",
    "test": "pnpm test:cleanup && pnpm test:db && pnpm test:embeddings && pnpm test:rag",
    "test:db": "export PLAYWRIGHT=True && pnpm exec playwright test --project=db --reporter=line",
    "test:cleanup": "lsof -ti:3100 | xargs kill -9 2>/dev/null || true",
    "test:routes": "pnpm test:cleanup && export PLAYWRIGHT=True && pnpm exec playwright test --project=routes --reporter=line",
    "test:e2e": "pnpm test:cleanup && export PLAYWRIGHT=True && pnpm exec playwright test --project=e2e --reporter=line",
    "test:all": "pnpm test:cleanup && export PLAYWRIGHT=True && pnpm exec playwright test --reporter=line",
    "test:embeddings": "export PLAYWRIGHT=True && pnpm exec playwright test --project=embeddings --reporter=line",
    "test:rag": "export PLAYWRIGHT=True && pnpm exec playwright test --project=rag --reporter=line",
    "eval:rag": "npx tsx scripts/eval-rag.ts"
  },
