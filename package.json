  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "knip": "npx knip",
    "lint": "npx ultracite check",
    "format": "npx ultracite fix && prettier --write \"**/*.md\"",
    "type-check": "tsc --noEmit",
    "check": "pnpm format && pnpm type-check",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "npx tsx lib/db/migrate.ts",
    "db:reset": "npx tsx lib/db/reset.ts",
    "db:parl:load": "npx tsx scripts/load-parliament-data.ts",
    "db:parl:embeds:gen": "npx tsx scripts/generate-embeddings.ts",
    "db:parl:embeds:gen:test": "npx tsx scripts/generate-test-embeddings.ts",
